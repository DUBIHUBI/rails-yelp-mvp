<div class="p-5">
<h1>Add a new restaurant review</h1>
<hr>
<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating%>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
<hr>
<%= link_to "Back", restaurant_path(@restaurant), class: "btn btn-danger" %>
</div>
